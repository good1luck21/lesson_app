<h1>Posts#index</h1>
<p>Find me in app/views/posts/index.html.erb</p>
<div>
    <%= form_for @post, url: posts_create_path do |f| %>
        <%= f.text_field :content %>
        <%= f.submit %>
    <% end %>
</div>

<div>
    <h2>投稿一覧</h2>
    <div id="post-list">
        <% @posts.each do|post| %>
            <p><%= link_to post.content,posts_edit_path(post.id) %></p>
        <% end %>
    </div>
</div>

